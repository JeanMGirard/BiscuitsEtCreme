{
  "name": "biscuits-et-creme",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/load_env.ts",
    "start": "ng serve",
    "build": "ng build",
    "build:cross-domains": "ng build --prod --base-href / --output-path dist/cross-domains",
    "build:prod": "ng build --prod --base-href https://biscuitsetcreme.ca/ --deploy-url https://biscuitsetcreme.ca/ --output-path dist/production",
    "build:fire": "ng build --prod --base-href https://biscuitsetcreme.web.app/ --deploy-url https://biscuitsetcreme.web.app/ --output-path dist/firebase",
    "build:gh": "ng build --prod --base-href https://dev.biscuitsetcreme.ca/ --deploy-url https://dev.biscuitsetcreme.ca/ --output-path dist/gh-pages",
    "build:gh2": "ng build --prod --base-href https://jeanmgirard.github.io/BiscuitsEtCreme/ --deploy-url https://jeanmgirard.github.io/BiscuitsEtCreme/ --output-path dist/",
    "deploy:gh": "npm run build:gh && ngh",
    "deploy:gh2": "npm run build:gh2 && ngh",
    "deploy:fire": "npm run build:fire && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@types/jquery": "^3.3.29",
    "angular-cli-ghpages": "^0.5.3",
    "bootstrap": "^4.3.1",
    "bootstrap-less-port": "^1.0.0",
    "circletype": "^2.3.0",
    "core-js": "^3.1.4",
    "file-saver": "^2.0.2",
    "firebase": "^6.2.0",
    "firebase-admin": "^8.1.0",
    "guid-typescript": "^1.0.9",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "promise-polyfill": "8.1.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.0.2",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.800.2",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "dotenv": "^8.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "yargs": "^13.2.4"
  }
}
