{
  "name": "biscuits-et-creme",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/load_env.ts",
    "start": "ng serve",
    "build": "ng build",
    "clean:cors": "npx rimraf dist/cross-domains",
    "clean:prod": "npx rimraf dist/production",
    "clean:fire": "npx rimraf dist/firebase",
    "build:all": "npx rimraf dist && npm run build:prod && npm run build:fire && npm run build:cross-domains",
    "build:cross-domains": "npm run clean:cors && ng build --prod --base-href / --output-path dist/cross-domains",
    "build:prod": "npm run clean:prod && ng build --prod --aot --base-href https://biscuitsetcreme.ca/ --deploy-url https://biscuitsetcreme.ca/ --output-path dist/production",
    "build:fire": "npm run clean:fire && ng build --prod --base-href https://biscuitsetcreme.web.app/ --deploy-url https://biscuitsetcreme.web.app/ --output-path dist/firebase",
    "build:gh": "ng build --prod --base-href https://dev.biscuitsetcreme.ca/ --deploy-url https://dev.biscuitsetcreme.ca/ --output-path dist/gh-pages",
    "build:gh2": "ng build --prod --base-href https://jeanmgirard.github.io/BiscuitsEtCreme/ --deploy-url https://jeanmgirard.github.io/BiscuitsEtCreme/ --output-path dist/",
    "deploy:gh": "npm run build:gh && ngh",
    "deploy:gh2": "npm run build:gh2 && ngh",
    "deploy:fire": "npm run build:fire && firebase deploy -p dist/firebase",
    "deploy": "npm run deploy:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "^8.0.3",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.3",
    "@types/jquery": "^3.5.5",
    "angular-cli-ghpages": "^0.6.2",
    "bootstrap": "^4.6.0",
    "bootstrap-less-port": "^1.0.0",
    "circletype": "^2.3.0",
    "core-js": "^3.9.1",
    "cross-fetch": "^3.1.0",
    "file-saver": "^2.0.5",
    "firebase": "^6.6.2",
    "firebase-admin": "^8.13.0",
    "guid-typescript": "^1.0.9",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "promise-polyfill": "8.1.3",
    "proxy-polyfill": "^0.3.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.14.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.1102.4",
    "@angular/cli": "^8.0.6",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/jasmine": "^3.3.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.2",
    "dotenv": "^8.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "yargs": "^13.3.2"
  }
}
